{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2022",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noUncheckedIndexedAccess": true
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}